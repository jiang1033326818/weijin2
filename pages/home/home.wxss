.navbar {
  display: flex;
  /* position: relative; */
   z-index: 1000;
  /* top: 724rpx;
  width: 100%; */
  background: white;
  height: 100rpx;
  position: relative
}

.navbar__item {
  position: relative;
  display: block;
  flex: 1;
  padding: 10rpx 0;
  text-align: center;
  font-size: 30rpx;
  height: 48rpx;
  line-height: 48rpx;
 z-index: 1000;
  /* <!-- NavBar的总高度为：height + padding-top + padding-bottom = 68rpx --> */
}

.navbar__item_on {
  /* color: #16B13A; */
  color: #009DFF;
    transition: all 0.3s;
  border-bottom: 10rpx solid #009DFF;
  border-top:3rpx solid #ddd;

  /* color: #494949; */
}

.navbar__slider {
  position: absolute;
  display: block;
  content: " ";
  left: 0;
  bottom: 0;
  height: 3px;
  /* background-color: #16B13A; */
  background-color: #009DFF;
  transition: transform .3s;

}

.navbar__title{
  display: inline-block;
  font-size: 30rpx;
  font-weight: 500;
  max-width: 8em;
  text-align: center;
}
